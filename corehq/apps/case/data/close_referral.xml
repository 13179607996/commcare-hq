<?xml version='1.0' ?>
<system version="1" uiVersion="1" xmlns:jrm="http://openrosa.org/jr/xforms" xmlns="http://commcarehq.org/case">
    <meta>
        <deviceID />
        <timeStart>%(time)s</timeStart>
        <timeEnd>%(time)s</timeEnd>
        <username>system</username>
        <userID />
        <uid>%(uid)s</uid>
    </meta>
    <case>
        <case_id>%(case_id)s</case_id>
        <user_id />
        <date_modified>%(time)s</date_modified>
        <referral>
            <referral_id>%(referral_id)s</referral_id>
            <update>
                <referral_type>%(referral_type)s</referral_type>
                <date_closed>%(time)s</date_closed>
            </update>
        </referral>
    </case>
</system>